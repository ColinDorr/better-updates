{
  "name": "colin-dorr/better-updates",
  "description": "This Craft CMS plugin helps keep track of updates and license statuses.",
  "type": "craft-plugin",
  "keywords": ["craft", "craft cms", "updates", "tracking", "mailing", "mail"],
  "license": "MIT",
  "authors": [
    {
      "name": "Colin Dorr",
      "homepage": "https://github.com/ColinDorr"
    }
  ],
  "support": {
    "issues": "https://github.com/ColinDorr/better-updates/issues?state=open",
    "source": "https://github.com/ColinDorr/better-updates",
    "docs": "https://github.com/ColinDorr/better-updates/blob/main/README.md"
  },
  "require": {
    "craftcms/cms": "^5.0 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "ColinDorr\\BetterUpdates\\": "src/"
    }
  },
  "extra": {
    "name": "Better Updates",
    "handle": "better-updates"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}